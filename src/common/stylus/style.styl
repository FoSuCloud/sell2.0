@import "variables"

@font-face
  font-family 'sell-icon'
  src url(icomoon-font-path + '/sell-icon.eot?lcfxw3')
  src url(icomoon-font-path + '/sell-icon.eot?lcfxw3#iefix') format('embedded-opentype'), url(icomoon-font-path + '/sell-icon.ttf?lcfxw3') format('truetype'), url(icomoon-font-path + '/sell-icon.woff?lcfxw3') format('woff'), url(icomoon-font-path + '/sell-icon.svg?lcfxw3#sell-icon') format('svg')
  font-weight normal
  font-style normal

[class^="icon-"], [class*=" icon-"]
  font-family 'sell-icon' !important
  speak none
  font-style normal
  font-weight normal
  font-variant normal
  text-transform none
  line-height 1

  /* Better Font Rendering =========== */
  -webkit-font-smoothing antialiased
  -moz-osx-font-smoothing grayscale

