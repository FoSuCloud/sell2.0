@import "variables"

@font-face
  font-family 'sell-icon'
  src url(icomoon-font-path + '/sell-icon.eot?li97yd')
  src url(icomoon-font-path + '/sell-icon.eot?li97yd#iefix') format('embedded-opentype'), url(icomoon-font-path + '/sell-icon.ttf?li97yd') format('truetype'), url(icomoon-font-path + '/sell-icon.woff?li97yd') format('woff'), url(icomoon-font-path + '/sell-icon.svg?li97yd#sell-icon') format('svg')
  font-weight normal
  font-style normal
  font-display block

[class^="icon-"], [class*=" icon-"]
  font-family 'sell-icon' !important
  speak none
  font-style normal
  font-weight normal
  font-variant normal
  text-transform none
  line-height 1

  /* Better Font Rendering =========== */
  -webkit-font-smoothing antialiased
  -moz-osx-font-smoothing grayscale

